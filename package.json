{
  "name": "shadow-fns",
  "version": "0.1.5",
  "description": "A javascript function library.",
  "main": "lib/index.js",
  "module": "src/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "test": "mocha --require @babel/register",
    "cover": "yarn cover:unit && yarn cover:report && yarn cover:check",
    "cover:unit": "nyc npm test",
    "cover:report": "nyc report --reporter=lcov",
    "cover:check": "nyc check-coverage --branches=100 --functions=100 --lines=100 --statements=100",
    "coveralls": "nyc npm test && yarn cover:check && nyc report --reporter=text-lcov | coveralls",
    "build": "yarn clean && yarn build:src && yarn build:umd:min && yarn build:doc && yarn build:types",
    "clean": "rm -rf ./dist ./lib",
    "build:src": "babel src --out-dir lib",
    "build:umd": "MODE=development webpack",
    "build:umd:min": "MODE=production webpack",
    "build:doc": "node scripts/build-doc.js",
    "build:types": "node scripts/build-types.js"
  },
  "keywords": [
    "shadow-fns",
    "functional",
    "pure",
    "currying",
    "point-free"
  ],
  "files": [
    "dist",
    "lib",
    "src",
    "types"
  ],
  "author": "shadow",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jinghua000/shadow-fns.git"
  },
  "bugs": {
    "url": "https://github.com/jinghua000/shadow-fns/issues"
  },
  "homepage": "https://github.com/jinghua000/shadow-fns",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "chalk": "^3.0.0",
    "coveralls": "^3.0.7",
    "current-path-join": "^0.1.0",
    "dmd": "^4.0.4",
    "fs-extra": "^8.1.0",
    "jsdoc-to-markdown": "^5.0.1",
    "mocha": "^6.1.3",
    "nyc": "^14.1.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2"
  }
}
